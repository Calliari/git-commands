#Merge dev into master


# E.X. 1

git checkout dev
git pull origin dev
git checkout master
git pull origin master
git merge dev
git push origin master

# get back to dev branch
git checkout dev


# E.X. 2

git checkout dev;
git pull origin dev;
git checkout master;
git pull origin master;
commitmessage="Merge branch 'dev' into 'master'. Get all updated code from 'dev' branch into 'master' branch";
git merge dev --no-ff --no-edit -m "$commitmessage";
git push origin master;

#change to dev branch
git checkout dev;
echo 'done!';
